import requests

def main():
    webhook_url = "https://discord.com/api/webhooks/1369326430676062369/YUsDsMpIcCU_nDBJha0AOYICNW3cME7GAwFMVEWaAubHTvOl-244GniKIi4cxjIGdqYT"
    nachricht = "Hallo von deinem Python-Script! üéâ"

    data = {
        "content": nachricht
    }

    response = requests.post(webhook_url, json=data)

    if response.status_code == 204:
        print("‚úÖ Nachricht erfolgreich gesendet.")
    else:
        print(f"‚ùå Fehler: {response.status_code} - {response.text}")

if __name__ == "__main__":
    main()
